{
    "name": "Traggo",
    "id": "traggo",
    "packaging_format": 1,
    "description": {
        "en": "Self-hosted tag-based time tracking",
        "fr": "self-hosted tag-based time tracking"
    },
    "version": "0.2.3~ynh1",
    "url": "https://traggo.net",
    "upstream": {
        "license": "GPL-3.0",
        "website": "https://traggo.net",
        "admindoc": "https://traggo.net/config/",
        "code": "https://github.com/traggo/server"
    },
    "license": "GPL-3.0",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
